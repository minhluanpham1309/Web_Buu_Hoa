<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - VĂN PHÒNG CÔNG CHỨNG BỬU HÒA</title>
	<script type="application/x-javascript">
		addEventListener("load", function () {
			setTimeout(hideURLbar, 0);
		}, false);

		function hideURLbar() {
			window.scrollTo(0, 1);
		}
	</script>
	<link href="~/Assets/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
	<link rel="stylesheet" href="~/Assets/Client/css/style.css" type="text/css" media="all" />
	<link href="~/Assets/Client/css/font-awesome.css" rel="stylesheet">
	<link href="//fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
	@*@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
	<!-- banner -->
	<div class="banner" id="home">
		<!-- agileinfo-dot -->
		<div class="agileinfo-dot">
			<div class="head">
				<div class="navbar-top">
					<!-- Brand and toggle get grouped for better mobile display -->
					<div class="navbar-header">
						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<div class="navbar-brand logo ">
							<h1>@Html.ActionLink("VĂN PHÒNG CÔNG CHỨNG BỬU HÒA", "Index", "Home", new { area = "" }, new { @class = "" }) <i class="fa fa-balance-scale" aria-hidden="true"></i></h1>
						</div>

					</div>

					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
						<ul class="nav navbar-nav link-effect-4">
							<li class="active">@Html.ActionLink("Home", "Index", "Home") </li>
							<li>@Html.ActionLink("About", "About", "Home") </li>
							<li>@Html.ActionLink("Contract", "Contract", "Home") </li>
							<li>@Html.ActionLink("Contact", "Contact", "Home")</li>
						</ul>
					</div>
					<!-- /.navbar-collapse -->
				</div>
			</div>
			<div class="w3layouts-banner-slider">
				<div class="container">
					<div class="slider">
						<div class="callbacks_container">
							<ul class="rslides callbacks callbacks1" id="slider4">
								<li>
									<div class="agileits-banner-info">
										<h3>
											International
											<div class="border"></div> <span>Law Firm</span>
										</h3>
										<div class="w3-button">
											<div class="w3ls-button">
												<span class="scroll hvr-shutter-out-vertical">@Html.ActionLink("About", "About", "Home")</span>
												@*<a href="#about" class="scroll hvr-shutter-out-vertical">About</a>
		</div>*@
												<div class="w3l-button">
													<span class="scroll hvr-shutter-out-vertical">@Html.ActionLink("Contact", "Contact", "Home")</span>
													@*<a href="#contact" class="scroll hvr-shutter-out-vertical">Contact</a>*@
												</div>
												<div class="clearfix"> </div>
											</div>
										</div>
									</div>
								</li>
								<li>
									<div class="agileits-banner-info">
										<h3>
											Welcome
											<div class="border"></div> <span>to the Legal Adviser</span>
										</h3>
										<div class="w3-button">
											<div class="w3ls-button">
												<span class="scroll hvr-shutter-out-vertical">@Html.ActionLink("About", "About", "Home")</span>
												@*<a href="#about" class="scroll hvr-shutter-out-vertical">About</a>
													</div>*@
												<div class="w3l-button">
													<span class="scroll hvr-shutter-out-vertical">@Html.ActionLink("Contact", "Contact", "Home")</span>
													@*<a href="#contact" class="scroll hvr-shutter-out-vertical">Contact</a>*@
												</div>
												<div class="clearfix"> </div>
											</div>
										</div>
								</li>
								<li>
									<div class="agileits-banner-info">
										<h3>
											International
											<div class="border"></div> <span>Law Firm</span>
										</h3>
										<div class="w3-button">
											<div class="w3ls-button">
												<span class="scroll hvr-shutter-out-vertical">@Html.ActionLink("About", "About", "Home")</span>
												@*<a href="#about" class="scroll hvr-shutter-out-vertical">About</a>
													</div>*@
												<div class="w3l-button">
													<span class="scroll hvr-shutter-out-vertical">@Html.ActionLink("Contact", "Contact", "Home")</span>
													@*<a href="#contact" class="scroll hvr-shutter-out-vertical">Contact</a>*@
												</div>
												<div class="clearfix"> </div>
											</div>
										</div>
								</li>
							</ul>
						</div>
						<div class="clearfix"> </div>

					</div>
				</div>
			</div>
		</div>
		<!-- //agileinfo-dot -->
	</div>
	<!-- //banner -->

	@RenderBody()
	<hr />

	<!-- footer -->
	<div class="footer">
		<div class="container">
			<div class="agileinfo_footer_bottom">
				<div class="col-md-6 agileinfo_footer_bottom_grid">
					<h6>Links</h6>
					<ul class="tag2 tag_agileinfo">
						<li>@Html.ActionLink("Home", "Index", "Home", new { @class = "scroll" })</li>
						<li>@Html.ActionLink("About", "About", "Home", new { @class = "scroll" })</li>
						<li>@Html.ActionLink("Contract", "Contract", "Home", new { @class = "scroll" })</li>
						<li>@Html.ActionLink("Contact", "Contact", "Home", new { @class = "scroll" })</li>
					</ul>
				</div>
				<div class="col-md-6 agileinfo_footer_bottom_grid">
					<h6>Follow Us</h6>
					<div class="w3l-social">
						<ul>
							<li><a href="#"><i class="fa fa-facebook"></i></a></li>
							<li><a href="#"><i class="fa fa-twitter"></i></a></li>
							<li><a href="#"><i class="fa fa-rss"></i></a></li>
						</ul>
					</div>
				</div>
				<div class="clearfix"> </div>
			</div>
			<div class="agileinfo_footer_bottom1">

				<p>© 2018. All rights reserved | Design by IT Pham</p>
			</div>
		</div>
	</div>
	<!-- //footer -->


	<script src="~/Assets/js/jquery-2.1.4.min.js"></script>
	<script src="~/Assets/js/bootstrap.js"></script>
	@*@Scripts.Render("~/bundles/jquery")*@
	@*@Scripts.Render("~/bundles/bootstrap")*@

	<script src="~/Assets/Client/js/bars.js"></script>
	<script src="~/Assets/Client/js/jarallax.js"></script>
	<script src="~/Assets/Client/js/SmoothScroll.min.js"></script>
	<script src="~/Assets/Client/js/easing.js"></script>
	<script src="~/Assets/Client/js/responsiveslides.min.js"></script>
	<script src="~/Assets/Client/js/move-top.js"></script>
	<script src="~/Assets/Client/js/Custom_Client.js"></script>


	@RenderSection("scripts", required: false)
</body>
</html>
